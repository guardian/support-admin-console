{
  "name": "cdk",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test-update": "jest -u",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "lint": "eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
    "synth": "cdk synth --path-metadata false --version-reporting false",
    "diff": "cdk diff --path-metadata false --version-reporting false"
  },
  "devDependencies": {
    "@guardian/cdk": "61.3.2",
    "@guardian/eslint-config-typescript": "10.0.1",
    "@types/jest": "^27.4.1",
    "@types/node": "18.18.0",
    "aws-cdk": "2.1005.0",
    "aws-cdk-lib": "2.189.1",
    "constructs": "10.3.0",
    "eslint": "8.56.0",
    "jest": "29.7.0",
    "prettier": "^3.7.4",
    "source-map-support": "^0.5.20",
    "ts-jest": "29.4.6",
    "tslib": "2.8.1",
    "typescript": "5.3.3"
  },
  "resolutions": {
    "json5": "^2.2.2"
  },
  "packageManager": "pnpm@8.15.7"
}
